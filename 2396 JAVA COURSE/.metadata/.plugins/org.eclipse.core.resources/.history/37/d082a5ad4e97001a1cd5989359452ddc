public class staticBinding { 
	public static class supercock { 
		static void print() 
		{ 
			System.out.println("print in superclass."); 
		} 
	} 
	public static class subclass extends supercock { 
		static void print() 
		{ 
			System.out.println("print in subclass."); 
		} 
	} 

	public static void main(String[] args) 
	{ 
		supercock A = new supercock(); 
		supercock B = new subclass(); 
		A.print(); 
		B.print(); 
	} 
} 
